{
  "name": "mesh-light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#2e2e2e"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#2e2e2e",
        "background": "#f5f5f5"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "comment.line"],
      "settings": {
        "foreground": "#9baa90",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Doc Comments",
      "scope": ["comment.line.documentation", "comment.line.documentation.module"],
      "settings": {
        "foreground": "#7a9a6a",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#a07872"
      }
    },
    {
      "name": "Keywords Control",
      "scope": ["keyword.control.mesh"],
      "settings": {
        "foreground": "#4a4a60",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords Declaration",
      "scope": ["keyword.declaration.mesh"],
      "settings": {
        "foreground": "#4a4a60",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords Operator",
      "scope": ["keyword.operator.mesh"],
      "settings": {
        "foreground": "#585868",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#708098",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#5a8580"
      }
    },
    {
      "name": "Constants and Booleans",
      "scope": ["constant", "constant.language", "constant.numeric"],
      "settings": {
        "foreground": "#907098"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#3a3a3a"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.pipe",
        "keyword.operator.arrow",
        "keyword.operator.annotation",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.range",
        "keyword.operator.diamond",
        "keyword.operator.concat",
        "keyword.operator.arrow.fat",
        "keyword.operator.try",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#707080"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#a0a0a0"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#7a8a68"
      }
    },
    {
      "name": "Interpolation",
      "scope": ["meta.interpolation", "punctuation.section.interpolation"],
      "settings": {
        "foreground": "#707080"
      }
    }
  ]
}
